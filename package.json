{
  "name": "dragon-ui",
  "version": "1.0.0-alpha.18",
  "description": "基于 React 的桌面端UI库",
  "tags": [
    "dragon-ui",
    "react",
    "zhongan"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "dragon",
    "dragon-ui",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "web"
  ],
  "license": "MIT",
  "repository": "git://github.com/JeromeLin/dragon-ui",
  "bugs": {
    "url": "https://github.com/JeromeLin/dragon-ui"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config scripts/webpack/config.dev.js",
    "clean": "rm -rf lib es dist .temp",
    "build:es": "tsc --outDir es -d",
    "build:lib": "tsc --outDir lib -d && tsc --outDir .temp --jsx preserve && babel .temp --out-dir lib --presets=env,react,stage-0 --plugins=transform-runtime && rm -rf .temp",
    "build:umd": "webpack --config scripts/webpack/config.umd.js && NODE_ENV=production webpack --config scripts/webpack/config.umd.js",
    "build:css": "cd scripts/gulp && gulp",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:css",
    "site:build": "rm -rf assets && NODE_ENV=production webpack --config scripts/webpack/config.site.js",
    "site": "npm run site:build && gh-pages -d assets",
    "lint:ts": "tslint -c tslint.json 'components/**/*.{ts,tsx}'",
    "lint:js": "eslint examples --ext '.js,.jsx'",
    "lint:style": "stylelint styles/**/*.scss examples/styles/**/*.scss --syntax scss",
    "lint:style:fix": "stylelint --fix styles/**/*.scss examples/styles/**/*.scss --syntax scss",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:style",
    "precommit": "lint-staged",
    "test": "cross-env TZ=America/New_York jest --coverage",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "docs": "cd docs && gitbook install && gitbook serve",
    "authors": "git log --format='%aN <%aE>' | sort -u | grep -v 'users.noreply.github.com' > AUTHORS",
    "release": "bash scripts/release.sh"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "css-animation": "^1.2.5",
    "hoist-non-react-statics": "^3.0.1",
    "rc-animate": "^3.0.0-rc.5",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/classnames": "^2.2.3",
    "@types/enzyme-to-json": "^1.5.0",
    "@types/jest": "^21.1.5",
    "@types/react": "16.4.2",
    "@types/react-dom": "^16.0.2",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^5.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babel-standalone": "^6.26.0",
    "codemirror": "^5.38.0",
    "core-js": "^2.5.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "enzyme-to-json": "^3.2.0",
    "eslint": "^4.19.1",
    "eslint-config-za": "^1.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "3.0.2",
    "fastclick": "^1.0.6",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-filesize": "0.0.6",
    "gulp-modify-css-urls": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "marked": "^0.4.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^2.1.5",
    "pre-commit": "^1.2.2",
    "prismjs": "^1.15.0",
    "prop-types": "^15.5.10",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.3.11",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.4.2",
    "react-transition-group": "^2.2.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "ts-jest": "^20.0.7",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/components"
    ],
    "setupFiles": [
      "<rootDir>/scripts/jest/environment.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jest/setup.js",
    "testRegex": "/__tests__/.*(\\.jsx|[^d]\\.ts)$",
    "collectCoverageFrom": [
      "components/**/*.{ts,tsx}",
      "!components/*/*.native.{ts,tsx}",
      "!components/*/PropsType.{ts,tsx}",
      "!components/**/style/*.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/scripts/jest/preprocessor",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "components/**/*.{ts,tsx}": "lint:ts",
    "{components,examples}/**/*.{js,jsx}": "lint:js",
    "{styles,examples}/**/*.scss": "lint:style"
  },
  "sideEffects": [
    "dist/*",
    "components/style/*",
    "components/**/style/*",
    "es/**/style/*",
    "lib/**/style/*",
    "examples/**"
  ]
}
